<AnchorPane maxHeight="650.0" maxWidth="1280.0" minHeight="650.0" minWidth="1280.0" prefHeight="650.0" prefWidth="1280.0" styleClass="black-color" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.softablitz.Controller">
    <children>
        <AnchorPane prefHeight="27.0" prefWidth="584.0">
            <children>
                <JFXSlider fx:id="bottomToUpCrop" layoutY="79.0" orientation="VERTICAL" prefHeight="558.0" prefWidth="30.0" />
                <JFXSlider fx:id="leftToRightCrop" layoutX="14.0" layoutY="72.0" prefHeight="14.0" prefWidth="1037.0" />
                <JFXSlider fx:id="topToBottomCrop" layoutX="1041.0" layoutY="79.0" orientation="VERTICAL" prefHeight="558.0" prefWidth="20.0">
               <effect>
                  <Lighting>
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
<!--                        <Light.Distant />-->
                     </light>
                  </Lighting>
               </effect></JFXSlider>
                <AnchorPane fx:id="imageViewPane" layoutX="45.0" layoutY="99.0" maxHeight="693.0" maxWidth="1062.0" minHeight="381.0" minWidth="-Infinity" prefHeight="517.0" prefWidth="973.0">
                    <effect>
                  <Glow level="0.13">
                     <input>
                        <ColorAdjust brightness="-1.0" />
                     </input>
                  </Glow>
                    </effect>
                </AnchorPane>
                <MenuBar layoutY="1.0" prefHeight="25.0" prefWidth="1280.0" styleClass="grey-color" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="1.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#openFile" text="Open" />
                                <MenuItem mnemonicParsing="false" onAction="#fileButtonClicked" text="Save" />
                                <MenuItem mnemonicParsing="false" text="Save as" />
                                <MenuItem mnemonicParsing="false" text="Exit" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="578.0" prefHeight="622.0" prefWidth="207.0" styleClass="grey-color" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <children>
                <JFXButton layoutY="14.0" onMouseClicked="#cropButtonPressed" prefHeight="39.0" prefWidth="203.0" styleClass="grey-color" text="   Crop" textAlignment="JUSTIFY" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/cropicon2.png" />
                     </image>
                     <effect>
                        <ColorAdjust brightness="0.92" contrast="1.0" hue="1.0" saturation="1.0">
                           <input>
                              <Glow level="0.9" />
                           </input>
                        </ColorAdjust>
                     </effect>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton layoutY="62.0" onMouseClicked="#cropButtonPressed" prefHeight="39.0" prefWidth="203.0" styleClass="grey-color" text=" Brightness" textAlignment="JUSTIFY" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="23.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/brightnessicon2.png" />
                     </image>
                     <effect>
                        <ColorAdjust brightness="0.92" contrast="1.0" hue="1.0" saturation="1.0">
                           <input>
                              <Glow level="0.9" />
                           </input>
                        </ColorAdjust>
                     </effect>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton layoutY="116.0" onMouseClicked="#cropButtonPressed" prefHeight="39.0" prefWidth="203.0" styleClass="grey-color" text="   Contrast" textAlignment="JUSTIFY" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/contrasticon2.png" />
                     </image>
                     <effect>
                        <ColorAdjust brightness="0.92" contrast="1.0" hue="1.0" saturation="1.0">
                           <input>
                              <Glow level="0.9" />
                           </input>
                        </ColorAdjust>
                     </effect>
                  </ImageView>
               </graphic>
            </JFXButton>
            </children>
         <effect>
            <ColorAdjust brightness="-0.19" contrast="1.0" />
         </effect>
      </AnchorPane>
      <JFXSlider fx:id="rightToLeftCrop" layoutX="13.0" layoutY="631.0" prefHeight="14.0" prefWidth="1037.0" />

    </children>
</AnchorPane>
