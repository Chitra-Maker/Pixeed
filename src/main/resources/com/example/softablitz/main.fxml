                <AnchorPane fx:id="imageViewPane" layoutX="45.0" layoutY="99.0" maxHeight="693.0" maxWidth="1062.0" minHeight="381.0" minWidth="-Infinity" prefHeight="517.0" prefWidth="973.0">
                    <children>
                        <ImageView fx:id="imageView" fitHeight="517.0" fitWidth="973.0" pickOnBounds="true" preserveRatio="true" />
                    </children>
                </AnchorPane>
                <MenuBar layoutY="1.0" prefHeight="25.0" prefWidth="1280.0" styleClass="grey-color" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="1.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#openFile" text="Open" />
                                <MenuItem mnemonicParsing="false" onAction="#fileButtonClicked" text="Save" />
                                <MenuItem mnemonicParsing="false" text="Save as" />
                                <MenuItem mnemonicParsing="false" text="Exit" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="578.0" prefHeight="623.0" prefWidth="211.0" styleClass="grey-color" AnchorPane.bottomAnchor="-1.0" AnchorPane.rightAnchor="-4.0">
            <children>
                <JFXButton layoutY="14.0" onMouseClicked="#cropButtonPressed" prefHeight="39.0" prefWidth="203.0" styleClass="grey-color" text="   Crop" textAlignment="JUSTIFY" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@icons/cropicon2.png" />
                            </image>
                            <effect>
                                <ColorAdjust brightness="0.92" contrast="1.0" hue="1.0" saturation="1.0">
                                    <input>
                                        <Glow level="0.9" />
                                    </input>
                                </ColorAdjust>
                            </effect>
                        </ImageView>
                    </graphic>
                </JFXButton>
                <JFXButton layoutY="62.0" onMouseClicked="#cropButtonPressed" prefHeight="39.0" prefWidth="203.0" styleClass="grey-color" text=" Brightness" textAlignment="JUSTIFY" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="23.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@icons/brightnessicon2.png" />
                            </image>
                            <effect>
                                <ColorAdjust brightness="0.92" contrast="1.0" hue="1.0" saturation="1.0">
                                    <input>
                                        <Glow level="0.9" />
                                    </input>
                                </ColorAdjust>
                            </effect>
                        </ImageView>
                    </graphic>
                </JFXButton>
                <JFXButton layoutY="116.0" onMouseClicked="#cropButtonPressed" prefHeight="39.0" prefWidth="203.0" styleClass="grey-color" text="   Contrast" textAlignment="JUSTIFY" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@icons/contrasticon2.png" />
                            </image>
                            <effect>
                                <ColorAdjust brightness="0.92" contrast="1.0" hue="1.0" saturation="1.0">
                                    <input>
                                        <Glow level="0.9" />
                                    </input>
                                </ColorAdjust>
                            </effect>
                        </ImageView>
                    </graphic>
                </JFXButton>
            <Button layoutX="60.0" layoutY="174.0" mnemonicParsing="false" onAction="#adjustHandle" text="Adjust" />
            <Slider fx:id="brightness" layoutX="16.0" layoutY="234.0" />
            <Slider fx:id="contrast" layoutX="16.0" layoutY="261.0" />
            <Slider fx:id="hue" layoutX="15.0" layoutY="289.0" />
            <Slider fx:id="saturation" layoutX="16.0" layoutY="322.0" />
            <Button fx:id="rotate" layoutX="14.0" layoutY="369.0" mnemonicParsing="false" onAction="#rotate90" text="RotateBy90" />
            <Button fx:id="rotate1" layoutX="102.0" layoutY="369.0" mnemonicParsing="false" onAction="#rotate180" text="RotateBy180" />
            </children>
        </AnchorPane>

    </children>
</AnchorPane>
